	<xsd:simpleType name="RESPA_FeePaymentCollectedByTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Party collecting fee.</xsd:documentation>
			<xsd:documentation>Party collecting fee</xsd:documentation>
			<xsd:documentation>term: RESPA Fee Payment Collected By Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Broker"/>
			<xsd:enumeration value="Investor"/>
			<xsd:enumeration value="Lender"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RESPA_FeePaymentPaidByTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Party responsible for payment of fee.</xsd:documentation>
			<xsd:documentation>term: RESPA Fee Payment Paid By Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Buyer"/>
			<xsd:enumeration value="Lender"/>
			<xsd:enumeration value="Seller"/>
			<xsd:enumeration value="ThirdParty"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RESPA_FeePaymentProcessTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Processing - typical buyer paid fee to be disclosed on GFE Closing - typical seller fee not disclosed on GFE</xsd:documentation>
			<xsd:documentation>term: RESPA Fee Payment Process Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Closing"/>
			<xsd:enumeration value="Processing"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RESPA_FeePercentBasisTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Defines the amount upon which the RESPA Fee Total Percent is calculated.</xsd:documentation>
			<xsd:documentation>term: RESPA Fee Percent Basis Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BaseLoanAmount"/>
			<xsd:enumeration value="OriginalLoanAmount"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="PropertyAppraisedValueAmount"/>
			<xsd:enumeration value="PurchasePriceAmount"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RESPA_FeeResponsiblePartyTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Is fee typically a responsibility of the Buyer or Seller</xsd:documentation>
			<xsd:documentation>term: RESPA Fee Responsible Party Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Buyer"/>
			<xsd:enumeration value="Seller"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RESPA_SummaryTotalFeesPaidToTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>A party to the transaction to whom a fees have been paid.</xsd:documentation>
			<xsd:documentation>term: RESPA Summary Total Fees Paid To Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Broker"/>
			<xsd:enumeration value="Investor"/>
			<xsd:enumeration value="Lender"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RateLockTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the method by which the rate lock period will be determined.</xsd:documentation>
			<xsd:documentation>term: Rate Lock Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BestEfforts"/>
			<xsd:enumeration value="Mandatory"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RealEstateAgentTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Describes the type of real estate agent.</xsd:documentation>
			<xsd:documentation>term: Real Estate Agent Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Listing">
				<xsd:annotation>
					<xsd:documentation>The agent that listed the property and represents the Seller.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Selling">
				<xsd:annotation>
					<xsd:documentation>The agent that sold the property and represents the Buyer (may also be known as the Buyers Agent).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RecordingEndorsementTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Describes the source and purpose of the recording endorsement information.</xsd:documentation>
			<xsd:documentation>term: Recording Endorsement Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AdditionalRecordingJurisdiction">
				<xsd:annotation>
					<xsd:documentation>The recording endorsement from any additional jurisdictions where the document is recorded.  Used only if a single document must be recorded in multiple jurisdictions and if the document submitted for recording already contains a completed PrimaryRecordingJurisdiction element.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CorrectiveEndorsement">
				<xsd:annotation>
					<xsd:documentation>An additional recording endorsement from the PrimaryRecordingJurisdicition.  Used when an error in a document is discovered after it has been recorded.  The document is corrected in accordance with jurisdictional requirements and resubmitted to the recording jurisdiction an additional time.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="PreviouslyRecordedReference">
				<xsd:annotation>
					<xsd:documentation>The recording endorsement information from a document that is already in the public record and is referenced in the body of a new document.  E.g.: a ‘Satisfaction of Mortgage’ would reference the recording endorsement information from the original mortgage in the body of the Satisfaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PrimaryRecordingJurisdiction">
				<xsd:annotation>
					<xsd:documentation>The recording endorsement from the jurisdiction where the document is being recorded.  If the document needs to be recorded in more than one jurisdiction, this would be the used by the first jurisdiction to record the document.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RecordingErrorSeverityTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>A description of the severity of the error</xsd:documentation>
			<xsd:documentation>term: Recording Error Severity Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Fatal">
				<xsd:annotation>
					<xsd:documentation>Request cannot be completed without corrective action</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NonFatal">
				<xsd:annotation>
					<xsd:documentation>Request completed – follow-up recommended on this transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>A severity indicator not included in the enumerated list
</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Warning">
				<xsd:annotation>
					<xsd:documentation>Request completed – fix error in future requests</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RecordingErrorTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>A categorical description of the error that occurred</xsd:documentation>
			<xsd:documentation>term: Recording Error Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BusinessRule">
				<xsd:annotation>
					<xsd:documentation>Indicates an error processing against a business rule
</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SchemaValidation">
				<xsd:annotation>
					<xsd:documentation>Indicates an error when validating against the published schema.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Image">
				<xsd:annotation>
					<xsd:documentation>Indicates an error with the image attachment</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>Indicates an error not included in the enumerated list</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Signature">
				<xsd:annotation>
					<xsd:documentation>Indicates an error with the electronic signature</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RecordingJurisdictionTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The type of Recording Jurisdiction in which the subject property is located.</xsd:documentation>
			<xsd:documentation>term: Recording Jurisdiction Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="County"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RecordingTransactionIdentifierTypeEnumerated">
