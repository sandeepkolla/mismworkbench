		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ServicingCommentTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Indicates the type of servicing comment.</xsd:documentation>
			<xsd:appinfo>Rename.  Replaces "Loan Comment Type".</xsd:appinfo>
			<xsd:documentation>term: Servicing Comment Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Collection"/>
			<xsd:enumeration value="CustomerService"/>
			<xsd:enumeration value="Default"/>
