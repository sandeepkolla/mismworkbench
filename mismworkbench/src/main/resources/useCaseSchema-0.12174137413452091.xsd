			<xsd:enumeration value="Appraiser"/>
			<xsd:enumeration value="Attorney"/>
			<xsd:enumeration value="CityTaxAuthority"/>
			<xsd:enumeration value="CountyTaxAuthority"/>
			<xsd:enumeration value="DocumentCustodian"/>
			<xsd:enumeration value="HazardInsuranceAgent"/>
			<xsd:enumeration value="HazardInsuranceCompany"/>
			<xsd:enumeration value="OptionalProductProvider"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="OtherTaxAuthority"/>
			<xsd:enumeration value="MortgageInsuranceCompany"/>
			<xsd:enumeration value="PoolCertificateHolder"/>
			<xsd:enumeration value="PropertyInspectionService"/>
			<xsd:enumeration value="PropertyManagementCompany"/>
			<xsd:enumeration value="RealEstateBroker"/>
			<xsd:enumeration value="TaxServiceProvider"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentBillingMethodTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Identifies the method used by the servicer to notify the mortgagor to remit the mortgage payment.</xsd:documentation>
			<xsd:documentation>term: Payment Billing Method Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BillOnReceipt">
				<xsd:annotation>
					<xsd:documentation>Statement sent based on transactions.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Coupons">
				<xsd:annotation>
					<xsd:documentation>Borrower is supplied with coupons</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NoBilling"/>
			<xsd:enumeration value="Statement">
				<xsd:annotation>
					<xsd:documentation>Statement is sent to Borrower according to Billing Statement Frequency.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="StatementAndCoupon">
				<xsd:annotation>
					<xsd:documentation>Statement is sent to Borrower according to Billing Statement Frequency and Borrower is supplied with coupons.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentBillingStatementFrequencyTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Identifies the frequency that statements are sent to the borrower.</xsd:documentation>
			<xsd:documentation>term: Payment Billing Statement Frequency Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Annual"/>
			<xsd:enumeration value="Monthly"/>
			<xsd:enumeration value="Quarterly"/>
			<xsd:enumeration value="Semiannual"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentDelinquencyStatusTypeEnumeration">
		<xsd:annotation>
			<xsd:documentation>Reports the delinquency status of the specific delinquency event.</xsd:documentation>
			<xsd:documentation>term: Payment Delinquency Status Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NotDelinquent"/>
			<xsd:enumeration value="MoreThan30Days"/>
			<xsd:enumeration value="MoreThan60Days"/>
			<xsd:enumeration value="MoreThan90Days"/>
			<xsd:enumeration value="MoreThan120Days"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentFrequencyTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the frequency of the mortgage payment.</xsd:documentation>
			<xsd:appinfo>  AUS  Enumerated Loan Features  Secondary Delivery Servicing AUS Servicing Transfer Secondary Pricing  Payment Frequency Type </xsd:appinfo>
			<xsd:documentation>term: Payment Frequency Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Biweekly"/>
			<xsd:enumeration value="Monthly"/>
			<xsd:enumeration value="Annual"/>
			<xsd:enumeration value="AtMaturity">
				<xsd:annotation>
					<xsd:documentation>Payment one time at maturity</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Quarterly"/>
			<xsd:enumeration value="Semiannual">
				<xsd:annotation>
					<xsd:documentation>Payment every six months</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Semimonthly">
				<xsd:annotation>
					<xsd:documentation>Payment twice a month</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Weekly"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentMethodTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>A description of the method of funds transfer to be used for the transaction.</xsd:documentation>
			<xsd:appinfo>Renamed.  Used to be "Payment Method Type".</xsd:appinfo>
			<xsd:documentation>term: Service Payment Method Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ACH">
				<xsd:annotation>
					<xsd:documentation>An Automated Clearing House transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="OnAccount">
				<xsd:annotation>
					<xsd:documentation>A prepaid account is to be charged for the transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CreditCard">
				<xsd:annotation>
					<xsd:documentation>A credit card is to be charged for the transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DebitCard">
				<xsd:annotation>
					<xsd:documentation>A debit card is to be debited for the transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>A form of payment not included in the enumerated list is to be used for the transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Voucher">
				<xsd:annotation>
					<xsd:documentation>A payment voucher is to be used for the transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Wire">
				<xsd:annotation>
					<xsd:documentation>A wire transfer transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentStateTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifices the state of the payment that is being reported.</xsd:documentation>
			<xsd:documentation>term: Payment State Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Current"/>
			<xsd:enumeration value="Pending"/>
			<xsd:enumeration value="Previous"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PayoffActionTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the type of action related to the liability payoff.</xsd:documentation>
			<xsd:documentation>term: Payoff Action Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="LenderOrderBothPayoffSubordinate"/>
			<xsd:enumeration value="LenderOrderPayoff"/>
			<xsd:enumeration value="LenderOrderSubordinate"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="TitleCompanyOrderBothPayoffSubordinate"/>
			<xsd:enumeration value="TitleCompanyOrderPayoff"/>
			<xsd:enumeration value="TitleCompanyOrderSubordinate"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PayoffApplicationSequenceTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the order in which the payoff is applied relative to the scheduled payment.</xsd:documentation>
			<xsd:documentation>term: Payoff Application Sequence Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="JustPayoff"/>
			<xsd:enumeration value="NormalPaymentThenPayoff"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PerChangeInterestRateRoundingTimingTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Describes when the rate is rounded relative to checking other rules.</xsd:documentation>
			<xsd:documentation>term: Per Change Interest Rate Rounding Timing Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="RoundRateAfterCheckingMinimumIndexRateMovement"/>
			<xsd:enumeration value="RoundRateBeforeCheckingMinimumIndexRateMovement"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PerChangePrincipalAndInterestPaymentAdjustmentCalculationMethodTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Describes the method employed to calculate the principal and interest payment of the loan.</xsd:documentation>
			<xsd:appinfo>To be used in lieu of Payment Adjustment Calculation Type</xsd:appinfo>
			<xsd:documentation>term: Per Change Principal And Interest Payment Adjustment Calculation Method Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AddDollarAmountToPreviousPrincipalAndInterest"/>
			<xsd:enumeration value="AddPercentBasedOnOriginalPrincipalAndInterest"/>
			<xsd:enumeration value="AddPercentBasedOnPreviousPrincipalAndInterest"/>
			<xsd:enumeration value="BasedOnInterestRateUsingAThrowawayRate">
				<xsd:annotation>
					<xsd:documentation>The principal and interest payment is based on an interest rate used solely for the calculation and not for accruing interest.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BasedOnNewInterestRate"/>
			<xsd:enumeration value="FHA245Formula"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PeriodicLateCountTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Indicates the time interval for the Late Count data. Possible intervals are First Year, Second Year, Third Year.</xsd:documentation>
			<xsd:documentation>term: Periodic Late Count Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CurrentOneToTwelveMonths">
				<xsd:annotation>
					<xsd:documentation>The most recent 12 months - months 1 to 12.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PreviousThirteenTo24Months">
				<xsd:annotation>
					<xsd:documentation>The next previous 12 months - months 13 to 24.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Previous25To36Months">
				<xsd:annotation>
					<xsd:documentation>The next previous 12 months - months 25 to 36.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PlatTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Identifies the type of plat (ie: subdivision, condominium, assessor's, certified survey map).</xsd:documentation>
			<xsd:documentation>term: Plat Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CertifiedSurveyMap"/>
			<xsd:enumeration value="Condominium"/>
			<xsd:enumeration value="PlannedUnitDevelopment"/>
			<xsd:enumeration value="Subdivision"/>
			<xsd:enumeration value="Timeshare"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PoolAccrualRateStructureTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the structure used to determine the accrual interest rate for the pool.</xsd:documentation>
			<xsd:documentation>term: Pool Accrual Rate Structure Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="StatedStructure"/>
			<xsd:enumeration value="WeightedAverageStructure"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PoolClassTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Indicates the type of pool and its guarantor (if any).</xsd:documentation>
			<xsd:documentation>term: Pool Class Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="GNMAI">
				<xsd:annotation>
					<xsd:documentation>Ginnie Mae I_Class A Pool</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="GNMAII">
				<xsd:annotation>
					<xsd:documentation>Ginnie Mae II_Class B Pool</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FNM">
				<xsd:annotation>
					<xsd:documentation>Fannie Mae Pool</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FRE">
				<xsd:annotation>
					<xsd:documentation>Freddie Mac Pool</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Private">
				<xsd:annotation>
					<xsd:documentation>Private Pool</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PoolInterestRateRoundingTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Indicates how the interest rate is rounded when a new interest rate is calculated for a pool of loans.</xsd:documentation>
			<xsd:documentation>term: Pool Interest Rate Rounding Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Down"/>
			<xsd:enumeration value="Nearest"/>
			<xsd:enumeration value="NoRounding"/>
			<xsd:enumeration value="Up"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PoolStructureTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Denotes the type of mortgage-backed security structure.</xsd:documentation>
			<xsd:documentation>Denotes the type of mortgage backed security structure.</xsd:documentation>
			<xsd:documentation>term: Pool Structure Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="InvestorDefinedMultipleLender"/>
			<xsd:enumeration value="LenderInitiatedMultipleLender"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="SingleLender"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PoolingMethodTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Indicates the timing of the remittance of the scheduled principal and interest payment from the servicer to the certificate holders of a particular pool.</xsd:documentation>
			<xsd:documentation>term: Pooling Method Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ConcurrentDate">
				<xsd:annotation>
					<xsd:documentation>Payments are remitted to investors 15 days after the loan scheduled due date.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="InternalReserve">
				<xsd:annotation>
					<xsd:documentation>Payments are remitted to investors 45 days after due date.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PreferredResponseFormatTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Designate the response format type for the service or product being requested. The format options are Text, PCL, PDF, XML or Other. If Other is selected, enter the format in Response Format Other.</xsd:documentation>
			<xsd:documentation>term: Preferred Response Format Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>Enter description in Response Format Other.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PCL">
				<xsd:annotation>
					<xsd:documentation>A report formated with graphic PCL (Print Control Language) commands.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PDF">
				<xsd:annotation>
					<xsd:documentation>A formatted text report in the Adobe PDF (Portable Document Format).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Text">
				<xsd:annotation>
					<xsd:documentation>A formatted text report.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="XML">
				<xsd:annotation>
					<xsd:documentation>XML format.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PreferredResponseMethodTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the method for delivering the service or product requested - Email, Fax, File, FTP, HTTP, etc. The destination information is listed in the Response Destination element.</xsd:documentation>
			<xsd:documentation>term: Preferred Response Method Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Fax">
				<xsd:annotation>
					<xsd:documentation>Supply a telephone number</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="File">
				<xsd:annotation>
					<xsd:documentation>Supply a path and file name</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FTP">
				<xsd:annotation>
					<xsd:documentation>Supply a URL</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HTTP">
				<xsd:annotation>
					<xsd:documentation>Supply a URL</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HTTPS">
				<xsd:annotation>
					<xsd:documentation>Supply a secure URL.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Mail">
				<xsd:annotation>
					<xsd:documentation>Supply a mailing address</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MessageQueue">
				<xsd:annotation>
					<xsd:documentation>Supply a queue name</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>Specify in Delivery Method Other element</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SMTP">
				<xsd:annotation>
					<xsd:documentation>Supply an email address.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VAN">
				<xsd:annotation>
					<xsd:documentation>Supply a mailbox ID</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrepaymentPenaltyAssessmentTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The method for calculating the prepayment penalty for the loan.</xsd:documentation>
			<xsd:documentation>term: Prepayment Penalty Assessment Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="FixedAmount"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="PercentageOfPrincipalBalance"/>
			<xsd:enumeration value="SpecifiedMonthsOfInterest"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrepaymentPenaltyCalculationBalanceTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The balance used to calculate the amount of the prepayment penalty.</xsd:documentation>
			<xsd:documentation>The balance used to calculate the amount of the Prepayment Penalty.</xsd:documentation>
			<xsd:documentation>term: Prepayment Penalty Calculation Balance Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AmountOfCurtailment"/>
			<xsd:enumeration value="CurrentPrincipalBalance"/>
			<xsd:enumeration value="OriginalBalance"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrepaymentPenaltyOptionTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Characteristic of prepayment penalty that indicates under what transaction type the penalty may be applied (hard/soft).</xsd:documentation>
			<xsd:documentation>term: Prepayment Penalty Option Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Hard">
				<xsd:annotation>
					<xsd:documentation>Standard prepay penalty applies in all situations.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Soft">
				<xsd:annotation>
					<xsd:documentation>Borrower may sell property without penalty. However, refinance will incur penalty.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrepaymentPenaltyPeriodTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The unit of time used for defining the duration of the prepayment penalty period (e.g., day, month).</xsd:documentation>
			<xsd:documentation>term: Prepayment Penalty Period Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Biweekly"/>
			<xsd:enumeration value="Day"/>
			<xsd:enumeration value="Month"/>
			<xsd:enumeration value="Quarter"/>
			<xsd:enumeration value="Semimonthly"/>
			<xsd:enumeration value="Week"/>
			<xsd:enumeration value="Year"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrepaymentPenaltyTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Identifies the type of early principal repayment eligible for a penalty charge.</xsd:documentation>
			<xsd:documentation>term: Prepayment Penalty Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Assumption"/>
			<xsd:enumeration value="EarlyPayoff"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="PrincipalCurtailment"/>
			<xsd:enumeration value="Refinance"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrepaymentPenaltyWaiverTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Denotes conditions under which the prepayment penalty is waived.</xsd:documentation>
			<xsd:documentation>term: Prepayment Penalty Waiver Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="HardshipSale"/>
			<xsd:enumeration value="None"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="ThirdPartySale"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PriceLockStatusTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The state of the price lock that is established between a borrower and a lender or between a lender and an investor.</xsd:documentation>
			<xsd:documentation>term: Price Lock Status Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CanceledOrWithdrawn"/>
			<xsd:enumeration value="Expired"/>
			<xsd:enumeration value="Locked"/>
			<xsd:enumeration value="ModificationRequested"/>
			<xsd:enumeration value="Modified"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Pending"/>
			<xsd:enumeration value="Rejected"/>
			<xsd:enumeration value="Requested">
				<xsd:annotation>
					<xsd:documentation>A mortgage that gives the mortgagee a security right over all other mortgages of the mortgaged property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrimaryMIAbsenceReasonTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the reason that primary mortgage insurance is not required or provided.</xsd:documentation>
			<xsd:documentation>term: Primary MI Absence Reason Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CharterParticipation"/>
			<xsd:enumeration value="DelinquencyDisposition"/>
			<xsd:enumeration value="InvestorPurchasedMortgageInsurance"/>
			<xsd:enumeration value="MICanceledBasedOnCurrentLTV"/>
			<xsd:enumeration value="NoMIBasedOnOriginalLTV"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="PledgedAsset"/>
			<xsd:enumeration value="PoolCoverage"/>
			<xsd:enumeration value="Repurchase"/>
			<xsd:enumeration value="SelfInsured"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrincipalAndInterestCalculationPaymentPeriodTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the payment period used to calculate the principal and interest payment.</xsd:documentation>
			<xsd:documentation>term: Principal And Interest Calculation Payment Period Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="30DayPaymentPeriod"/>
			<xsd:enumeration value="NumberOfDaysBetweenPayments"/>
			<xsd:enumeration value="NumberOfDaysInCalendarMonth"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrincipalAndInterestPaymentDecreaseCapTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Defines the optionality of a principal and interest payment cap when the principal and interest payment is adjusting downward.</xsd:documentation>
			<xsd:documentation>term: Principal And Interest Payment Decrease Cap Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Mandatory"/>
			<xsd:enumeration value="Optional"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrincipalAndInterestPaymentFinalRecastTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Describes the principal and interest recast behavior at the end of the loan term.</xsd:documentation>
			<xsd:documentation>term: Principal And Interest Payment Final Recast Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="None"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="RecastOnFinalPaymentChangeDateAndEveryMonthThereafter"/>
			<xsd:enumeration value="RecastOnceOnFinalPaymentChangeDate"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrincipalAndInterestPaymentIncreaseCapTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Defines the optionality of a principal and interest payment cap when the principal and interest payment is adjusting upward.</xsd:documentation>
			<xsd:documentation>term: Principal And Interest Payment Increase Cap Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Mandatory"/>
			<xsd:enumeration value="Optional"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrincipalBalanceCalculationMethodTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Indicates the method used to determine the principal balance to be used to compute the new principal and interest payment.</xsd:documentation>
			<xsd:documentation>term: Principal Balance Calculation Method Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CurrentPrincipalBalance">
				<xsd:annotation>
					<xsd:documentation>Use the current principal balance at the current payment due date.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ProjectedPrincipalBalance">
				<xsd:annotation>
					<xsd:documentation>Project the actual principal balance forward to the change date, normal method of calculation.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PriorPropertyTitleTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Borrowers answer to the question: How did you hold title to the home solely by yourself, jointly with your spouse, or jointly with another person? Collected on the URLA in Section VIII line m2.</xsd:documentation>
			<xsd:documentation>term: Prior Property Title Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="JointWithOtherThanSpouse">
				<xsd:annotation>
					<xsd:documentation>Use this answer for any property owned with any other person including spouse.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="JointWithSpouse">
				<xsd:annotation>
					<xsd:documentation>Use this answer for any property owned with a spouse.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Sole">
				<xsd:annotation>
					<xsd:documentation>Individual</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PriorPropertyUsageTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Borrowers answer to the question: What type of property did you own principal residence, second home, or investment property? Collected on the URLA in Section VIII line m1.</xsd:documentation>
			<xsd:documentation>term: Prior Property Usage Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Investment">
				<xsd:annotation>
					<xsd:documentation>A home owned for the purpose of generating income by renting the property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PrimaryResidence">
				<xsd:annotation>
					<xsd:documentation>Residence that the owner physically occupies and uses as his or her principal residence.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SecondHome">
				<xsd:annotation>
					<xsd:documentation>A property occupied by the owner for a portion of the year and is not the primary residence.</xsd:documentation>
					<xsd:appinfo>Replaces "SecondaryResidence" from Version 2.x.</xsd:appinfo>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ProductProviderTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the role of the provider of the defined offering of goods or services.</xsd:documentation>
			<xsd:documentation>term: Product Provider Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Investor"/>
			<xsd:enumeration value="Lender"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ProjectAttachmentTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the type of physical attachment, if any, between the dwelling units in the project.</xsd:documentation>
			<xsd:documentation>term: Project Attachment Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Attached"/>
			<xsd:enumeration value="Detached"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ProjectDesignTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>This field specifies the type of design for the multiple-unit buildings in a project.</xsd:documentation>
			<xsd:appinfo> Enumerated Project  Appraisal AUS  Project Design Type </xsd:appinfo>
			<xsd:documentation>term: Project Design Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="GardenProject"/>
			<xsd:enumeration value="HighriseProject"/>
			<xsd:enumeration value="MidriseProject"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="TownhouseRowhouse">
				<xsd:annotation>
					<xsd:documentation>One in a row of identical houses or having common wall. Attached to another unit via common wall (e.g., a brownstone.)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ProjectLegalStructureTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the form of ownership that defines the quality and quantity of ownership and rights to the individual unit owner.</xsd:documentation>
			<xsd:documentation>term: Project Legal Structure Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Condominium">
				<xsd:annotation>
					<xsd:documentation>A project that is legally formed as a condominium under the Condominium-Enabling Legislation of the state in which the project is located.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Cooperative">
				<xsd:annotation>
					<xsd:documentation>A project in which a corporation or business trust holds title to the property and issues shares of stock as evidence of ownership in the corporation or business trust. The corporation or business trust grants occupancy rights to the shareholder tenants through proprietary leases. </xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CommonInterestApartment">
				<xsd:annotation>
					<xsd:documentation>Any project or building that is owned by several owners as tenants in common or by a home owners association in which individuals have an undivided interest in a residential apartment building and land, and are the rights of exclusive occupancy of a specific apartment in the building.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyCurrentOccupancyTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the property occupancy status of a subject property.</xsd:documentation>
			<xsd:documentation>term: Property Current Occupancy Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Abandoned"/>
			<xsd:enumeration value="AdverseOccupied"/>
			<xsd:enumeration value="OwnerOccupied"/>
			<xsd:enumeration value="RemovedOrDestroyed"/>
			<xsd:enumeration value="TenantOccupied"/>
			<xsd:enumeration value="Unknown"/>
			<xsd:enumeration value="Vacant"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyEstateTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the ownership interest in the property.</xsd:documentation>
			<xsd:documentation>term: Property Estate Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="FeeSimple">
				<xsd:annotation>
					<xsd:documentation>The greatest possible interest a person can have in real estate, including the right to dispose of the property or pass it on to one's heirs.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Fractional">
				<xsd:annotation>
					<xsd:documentation>An shared interest among multiple parties in the single title or deed associated with a property. Fractional ownership generally restricts the owners usage of the property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Leasehold">
				<xsd:annotation>
					<xsd:documentation>An estate or interest in real property held by virtue of a lease.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyNeighborhoodLocationTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the urban, suburban, or rural nature of the location of the subject property.</xsd:documentation>
			<xsd:appinfo> Enumerated Property  Appraisal Secondary Pricing AUS SEC  Property Neighborhood Location Type </xsd:appinfo>
			<xsd:documentation>term: Property Neighborhood Location Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Rural"/>
			<xsd:enumeration value="Suburban"/>
			<xsd:enumeration value="Urban"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyOccupancyStatusTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Based on current property inspection result the occupancy status of the property</xsd:documentation>
			<xsd:appinfo> Enumerated Property  Servicing Servicing Transfer AUS SEC  Current Vacancy Status Type </xsd:appinfo>
			<xsd:documentation>term: Property Occupancy Status Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Occupied"/>
			<xsd:enumeration value="Unknown"/>
			<xsd:enumeration value="Vacant"/>
			<xsd:enumeration value="Abandoned">
				<xsd:annotation>
					<xsd:documentation>Real property to which the owner has voluntarily and intentionally relinquished ownership, possession, claim and control.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyUsageTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the usage intention of the borrower for the property.</xsd:documentation>
			<xsd:documentation>term: Property Usage Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Investment">
				<xsd:annotation>
					<xsd:documentation>A home owned for the purpose of generating income by renting the property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SecondHome">
				<xsd:annotation>
					<xsd:documentation>A property occupied by the owner for a portion of the year and is not the primary residence.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PrimaryResidence">
				<xsd:annotation>
					<xsd:documentation>Residence that the owner physically occupies and uses as his or her principal residence.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyValuationConclusionDependencyTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the appraisal's conclusion's dependency on future repairs or activities.</xsd:documentation>
			<xsd:appinfo>Renamed.  Used to be "Valuation Reconciliation Condition Of Appraisal Type".</xsd:appinfo>
			<xsd:documentation>term: Property Valuation Conclusion Dependency Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AsIs"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="SubjectToCompletionPerPlans"/>
			<xsd:enumeration value="SubjectToRepairsAndConditions"/>
			<xsd:enumeration value="SubjectToRepairsProvingUnnecessary"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyValuationFormTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the form or document used to provide the property valuation.</xsd:documentation>
			<xsd:documentation>term: Property Valuation Form Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AppraisalUpdateAndOrCompletionReport">
				<xsd:annotation>
					<xsd:documentation>FNM 1004D FRE 442</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DefinitionsStatementOfLimitingConditionsAndAppraisersCertification">
				<xsd:annotation>
					<xsd:documentation>FNM 1004B</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DesktopUnderwriterPropertyInspectionReport">
				<xsd:annotation>
					<xsd:documentation>FNM 2075</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EmployeeRelocationCouncil2001">
				<xsd:annotation>
					<xsd:documentation>ERC2001</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ExteriorOnlyInspectionIndividualCondominiumUnitAppraisalReport">
				<xsd:annotation>
					<xsd:documentation>FNM 1075 FRE 466</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ExteriorOnlyInspectionIndividualCooperativeInterestAppraisalReport">
				<xsd:annotation>
					<xsd:documentation>FNM 2095</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ExteriorOnlyInspectionResidentialAppraisalReport">
				<xsd:annotation>
					<xsd:documentation>FNM 2055 FRE2055</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Form261805">
				<xsd:annotation>
					<xsd:documentation>Used for VA loans</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Form268712"/>
			<xsd:enumeration value="IndividualCondominiumUnitAppraisalReport">
				<xsd:annotation>
					<xsd:documentation>FNM 1073, FRE 465</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IndividualCooperativeInterestAppraisalReport">
				<xsd:annotation>
					<xsd:documentation>FNM 2090</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LoanProspectorConditionAndMarketability">
				<xsd:annotation>
					<xsd:documentation>FRE 2070</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ManufacturedHomeAppraisalReport">
				<xsd:annotation>
					<xsd:documentation>FNM 1004C FRE 70B</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MarketConditionsAddendumToTheAppraisalReport">
				<xsd:annotation>
					<xsd:documentation>FNM 1004MC</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="OneUnitResidentialAppraisalFieldReviewReport">
				<xsd:annotation>
					<xsd:documentation>FNM 2000 FRE1032</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="SmallResidentialIncomePropertyAppraisalReport">
				<xsd:annotation>
					<xsd:documentation>FNM 1025, FRE 72</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TwoToFourUnitResidentialAppraisal">
				<xsd:annotation>
					<xsd:documentation>FNM 2000A, FRE 1072</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UniformResidentialAppraisalReport">
				<xsd:annotation>
					<xsd:documentation>FNM 1004, FRE 70</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyValuationMethodTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the method by which the property value was  assessed.</xsd:documentation>
			<xsd:documentation>term: Property Valuation Method Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AutomatedValuationModel"/>
			<xsd:enumeration value="BrokerPriceOpinion"/>
			<xsd:enumeration value="DesktopAppraisal">
				<xsd:annotation>
					<xsd:documentation>This appraisal valuation is commonly used for refinancing, home equity loans, and home equity lines of credit.  The appraiser determines the value of the property through public records, tax assessments and comparable sales history.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DriveBy"/>
			<xsd:enumeration value="FullAppraisal"/>
			<xsd:enumeration value="None"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="PriorAppraisalUsed"/>
			<xsd:enumeration value="TaxValuation"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyValuationTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies that the subject property valuation amount is the amount at origination or a subsequent evaluation.</xsd:documentation>
			<xsd:documentation>term: Property Valuation Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Original"/>
			<xsd:enumeration value="Subsequent"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyZoningCategoryTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the property zoning category. The zoning category is not the official legal description.</xsd:documentation>
			<xsd:appinfo> Enumerated Property  Appraisal AUS  Property Zoning Category Type </xsd:appinfo>
			<xsd:documentation>term: Property Zoning Category Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Agricultural"/>
			<xsd:enumeration value="Commercial"/>
			<xsd:enumeration value="Industrial"/>
			<xsd:enumeration value="NoZoning"/>
			<xsd:enumeration value="NonResidentialGrandfatheredResidential"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Residential"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PurchaseCreditSourceTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the party contributing credits to the transaction exclusive of the mortgage financing. This may be collected on the URLA in Section VII (Details of Transaction - Other Credits - Explain)</xsd:documentation>
			<xsd:appinfo>  AUS  Enumerated Purchase Credit  Secondary Delivery AUS  Purchase Credit Source Type </xsd:appinfo>
			<xsd:documentation>term: Purchase Credit Source Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BorrowerPaidOutsideClosing"/>
			<xsd:enumeration value="PropertySeller"/>
			<xsd:enumeration value="Lender"/>
			<xsd:enumeration value="NonParentRelative">
				<xsd:annotation>
					<xsd:documentation>A relative other than a parent.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BuilderDeveloper"/>
			<xsd:enumeration value="Employer"/>
			<xsd:enumeration value="FederalAgency"/>
			<xsd:enumeration value="LocalAgency"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Parent"/>
			<xsd:enumeration value="RealEstateAgent"/>
			<xsd:enumeration value="StateAgency"/>
			<xsd:enumeration value="UnrelatedFriend"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PurchaseCreditTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the general names (types) of credit on the transaction exclusive of financing. This may be collected on the URLA in Section VII (Details of Transaction - Other Credits - Explain).</xsd:documentation>
			<xsd:appinfo> Enumerated Purchase Credit  AUS SEC  Purchase Credit Type </xsd:appinfo>
			<xsd:documentation>term: Purchase Credit Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BuydownFund">
				<xsd:annotation>
					<xsd:documentation>Funds held for the purpose of subsidizing some part of the periodic P&amp;I in a specific pattern for a specific time period.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CommitmentOriginationFee"/>
			<xsd:enumeration value="EarnestMoney">
				<xsd:annotation>
					<xsd:documentation>A deposit made as a show of good faith by buyer to bind the conditions of a sale of real estate.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EmployerAssistedHousing">
				<xsd:annotation>
					<xsd:documentation>Funds provided to borrower by employer to offset some of the initial or ongoing costs of borrowers home.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="GiftOfEquity">
				<xsd:annotation>
					<xsd:documentation>Without consideration or promise of repayment, owner of property credits buyer with a specified dollar amount of equity. This is most often seen in inter-generation transfers in families, where parents will credit children with some portion of equity not conveyed in cash</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LeasePurchaseFund">
				<xsd:annotation>
					<xsd:documentation>Under a lease/purchase arrangement, owner of real estate agrees to credit some portion of rent towards down payment. This is the accumulated amount of such credits reflected at closing.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>Purchase credits of any type not otherwise listed; typically linked to a FREE TEXT description field.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PrivateMIMIPremiumVAFundingFeeRefund">
				<xsd:annotation>
					<xsd:documentation>Anticipated refund of unearned prepaid Private Mortgage Insurance, Mortgage Insurance Premium, or VA Funding Fee arising from the subject transaction that is accepted as a valid source of borrower funds to close the transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RelocationFunds">
				<xsd:annotation>
					<xsd:documentation>Funds provided to relocating borrower (usually by employer) to offset some of the initial or ongoing costs of borrowers home at the new location.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SweatEquity">
				<xsd:annotation>
					<xsd:documentation>Buyer/borrower agrees with owner (who is usually also builder/re-habber) that work done by Buyer/borrower to help complete the property will be credited as having specific value and will be done in lieu of some portion of the cash purchase price.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TradeEquity">
				<xsd:annotation>
					<xsd:documentation>Usually involves what the IRS calls a 1031 exchange of equity the buyer and seller hold in one or more other properties, in lieu of some portion of the cash purchase price</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="QualifyingRateTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Indicates what interest rate was used to qualify the borrower in correlation with the note rate.</xsd:documentation>
			<xsd:documentation>term: Qualifying Rate Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AtNoteRate"/>
			<xsd:enumeration value="AboveNoteRate"/>
			<xsd:enumeration value="BelowNoteRate"/>
			<xsd:enumeration value="BoughtDownRate"/>
			<xsd:enumeration value="OtherRateType"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="REOMarketingPartyTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Identifies the party responsible for marketing the property in case of default.</xsd:documentation>
			<xsd:documentation>term: REO Marketing Party Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Investor"/>
			<xsd:enumeration value="Lender"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Servicer"/>
			<xsd:enumeration value="Unknown"/>
			<xsd:enumeration value="Seller">
				<xsd:annotation>
					<xsd:documentation>The organization that sells the loan to the investor.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MortgageInsuranceCompany"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="REOPropertyDispositionStatusTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The applicants intended disposition of an owned real property. One source of this data is the URLA in Section VI (Schedule of Real Estate Owned).</xsd:documentation>
			<xsd:appinfo>Formerly "REO Property Disposition Status Type" in Version 2.x</xsd:appinfo>
			<xsd:documentation>The borrowers intended disposition of an owned property. Collected on the URLA in Section VI (Schedule of Real Estate Owned).</xsd:documentation>
			<xsd:documentation>term: Owned Property Disposition Status Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="PendingSale"/>
			<xsd:enumeration value="RetainForPrimaryOrSecondaryResidence"/>
			<xsd:enumeration value="RetainForRental"/>
			<xsd:enumeration value="Sold">
				<xsd:annotation>
					<xsd:documentation>Sale of property to be closed at or before closing of the subject property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RESPAFeeTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the general names (type) of fees associated with the Good Faith Estimate/HUD-1.</xsd:documentation>
			<xsd:documentation>term: RESPA Fee Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="203KArchitecturalAndEngineeringFee"/>
			<xsd:enumeration value="203KConsultantFee"/>
			<xsd:enumeration value="203KDiscountOnRepairs"/>
			<xsd:enumeration value="203KInspectionFee"/>
			<xsd:enumeration value="203KPermits"/>
			<xsd:enumeration value="203KSupplementalOriginationFee"/>
			<xsd:enumeration value="203KTitleUpdate"/>
			<xsd:enumeration value="AbstractOrTitleSearchFee"/>
			<xsd:enumeration value="AmortizationFee"/>
			<xsd:enumeration value="ApplicationFee"/>
			<xsd:enumeration value="AppraisalFee"/>
			<xsd:enumeration value="AppraisalDeskReviewFee">
				<xsd:annotation>
					<xsd:documentation>Fee charged for reviewing an appraisal report and performing some basic research on value without going out to the field to view the property or the comparable properties included in the appraisal report.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AppraisalFieldReviewFee">
				<xsd:annotation>
					<xsd:documentation>Fee charged for performing an appraisal review in the field – actually inspecting the property and comparable properties.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AssignmentFee"/>
			<xsd:enumeration value="AssignmentRecordingFee"/>
			<xsd:enumeration value="AssumptionFee"/>
			<xsd:enumeration value="AttorneyFee"/>
			<xsd:enumeration value="BankruptcyMonitoringFee">
				<xsd:annotation>
					<xsd:documentation>Monitoring or Proof of Claim Fee charged as a result of a bankruptcy filing by the Borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BondFee">
				<xsd:annotation>
					<xsd:documentation>Fee paid to a state or local housing agency to participate in a special lending program.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BondReviewFee"/>
			<xsd:enumeration value="CertificationFee">
				<xsd:annotation>
					<xsd:documentation>Fee charged to prepare and submit to the Home Owner’s, Condominium, or Co-Op Association Certification for the property securing the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CityCountyDeedTaxStampFee"/>
			<xsd:enumeration value="CityCountyMortgageTaxStampFee"/>
			<xsd:enumeration value="CLOAccessFee"/>
			<xsd:enumeration value="ClosingProtectionLetterFee">
				<xsd:annotation>
					<xsd:documentation>Fee charged for a statement or document issued by title insurance underwriters that sets forth an underwriter's responsibility for negligence, fraud and errors in closings performed by the underwriter's agents and approved attorneys.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CommitmentFee"/>
			<xsd:enumeration value="CopyOrFaxFee"/>
			<xsd:enumeration value="CourierFee"/>
			<xsd:enumeration value="CreditReportFee"/>
			<xsd:enumeration value="DeedRecordingFee"/>
			<xsd:enumeration value="DocumentPreparationFee"/>
			<xsd:enumeration value="DocumentaryStampFee"/>
			<xsd:enumeration value="ElectronicDocumentDeliveryFee">
				<xsd:annotation>
					<xsd:documentation>Fee for transmitting or receiving the loan documents through email or by some other electronic means.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EscrowServiceFee">
				<xsd:annotation>
					<xsd:documentation>Fee to set up and service the escrow account for the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EscrowWaiverFee"/>
			<xsd:enumeration value="FloodCertification"/>
			<xsd:enumeration value="GeneralCounselFee"/>
			<xsd:enumeration value="InspectionFee"/>
			<xsd:enumeration value="LoanDiscountPoints"/>
			<xsd:enumeration value="LoanOriginationFee"/>
			<xsd:enumeration value="MERSRegistrationFee">
				<xsd:annotation>
					<xsd:documentation>Fee charged for registering/transferring the loan onto the Mortgage Electronic Registration System.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ModificationFee"/>
			<xsd:enumeration value="MortgageBrokerFee"/>
			<xsd:enumeration value="MortgageRecordingFee"/>
			<xsd:enumeration value="MunicipalLienCertificateFee"/>
			<xsd:enumeration value="MunicipalLienCertificateRecordingFee"/>
			<xsd:enumeration value="NewLoanAdministrationFee"/>
			<xsd:enumeration value="NotaryFee"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="PayoffRequestFee">
				<xsd:annotation>
					<xsd:documentation>In refinances, fee paid to prior lender to prepare and provide a bookkeeping statement of how much is owed on the loan being refinanced.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PestInspectionFee"/>
			<xsd:enumeration value="ProcessingFee"/>
			<xsd:enumeration value="RealEstateCommission"/>
			<xsd:enumeration value="RedrawFee"/>
			<xsd:enumeration value="ReinspectionFee"/>
			<xsd:enumeration value="ReleaseRecordingFee"/>
			<xsd:enumeration value="RuralHousingFee"/>
			<xsd:enumeration value="SettlementOrClosingFee"/>
			<xsd:enumeration value="SigningAgentFee">
				<xsd:annotation>
					<xsd:documentation>Fee charged for the services of a signing agent who, as an accommodation of the borrower, closes the loan at a specific time or at a location requested by the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="StateDeedTaxStampFee"/>
			<xsd:enumeration value="StateMortgageTaxStampFee"/>
			<xsd:enumeration value="SubordinationFee">
				<xsd:annotation>
					<xsd:documentation>Fee paid to a lien holder with an existing lien on the property to subordinate its lien position to the Lender in the current loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SurveyFee"/>
			<xsd:enumeration value="TaxRelatedServiceFee"/>
			<xsd:enumeration value="TitleEndorsementFee">
				<xsd:annotation>
					<xsd:documentation>Fee charged for obtaining a title endorsement (for which the original lender and/or borrower are the beneficiaries) that relates to the closing of the mortgage loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TitleExaminationFee"/>
			<xsd:enumeration value="TitleInsuranceBinderFee"/>
			<xsd:enumeration value="TitleInsuranceFee"/>
			<xsd:enumeration value="UnderwritingFee"/>
			<xsd:enumeration value="WireTransferFee">
				<xsd:annotation>
					<xsd:documentation>Fee for wiring funds in connection with the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RESPASectionClassificationTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The section number on the Good Faith Estimate/HUD-1 associated with RESPA fee type.</xsd:documentation>
			<xsd:documentation>term: RESPA Section Classification Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="100:GrossAmountDueFromBorrower"/>
			<xsd:enumeration value="200:AmountsPaidByOrInBehalfOfBorrower"/>
			<xsd:enumeration value="300:CashAtSettlementFromToBorrower"/>
			<xsd:enumeration value="400:GrossAmountDueToSeller"/>
			<xsd:enumeration value="500:ReductionsInAmountDueSeller"/>
			<xsd:enumeration value="600:CashAtSettlementToFromSeller"/>
			<xsd:enumeration value="700:DivisionOfCommissions"/>
			<xsd:enumeration value="800:LoanFees"/>
			<xsd:enumeration value="900:LenderRequiredPaidInAdvance"/>
			<xsd:enumeration value="1000:ReservesDepositedWithLender"/>
			<xsd:enumeration value="1100:TitleCharges"/>
			<xsd:enumeration value="1200:RecordingAndTransferCharges"/>
			<xsd:enumeration value="1300:AdditionalSettlementCharges"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RESPASummaryTotalFeesPaidByTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>A party to the transaction by whom a fee or fees have been designated paid.</xsd:documentation>
			<xsd:documentation>term: RESPA Summary Total Fees Paid By Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Broker"/>
			<xsd:enumeration value="Buyer"/>
			<xsd:enumeration value="Investor"/>
			<xsd:enumeration value="Lender"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Seller"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RESPA_FeePaidToTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Identifies the category or type of payee to which the fee will be paid</xsd:documentation>
			<xsd:documentation>term: RESPA Fee Paid To Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AffiliateProvider">
				<xsd:annotation>
					<xsd:documentation>Fee is being paid to a third party provider who is an affiliate of the Lender or Broker.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Broker"/>
			<xsd:enumeration value="Investor"/>
			<xsd:enumeration value="Lender"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Provider">
				<xsd:annotation>
					<xsd:documentation>Fee is being paid to a third party provider.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
