	<xsd:simpleType name="PRIARequestTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>A description of the action being requested</xsd:documentation>
			<xsd:documentation>term: PRIA Request Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="RecordDocuments">
				<xsd:annotation>
					<xsd:documentation>The request is that documents be recorded</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="StatusUpdate">
				<xsd:annotation>
					<xsd:documentation>The request is for a status update on a previously submitted request</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FeeQuote">
				<xsd:annotation>
					<xsd:documentation>The request is for the county to provide a fee quote based on information contained in the request</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>The request is for an action not listed in the enumerated list</xsd:documentation>
				</xsd:annotation>
