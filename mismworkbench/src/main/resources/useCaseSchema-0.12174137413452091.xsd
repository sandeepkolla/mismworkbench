			<xsd:enumeration value="LienHolder"/>
			<xsd:enumeration value="LoanDeliveryFilePreparer"/>
			<xsd:enumeration value="LoanOriginationCompany"/>
			<xsd:enumeration value="LoanOriginator"/>
			<xsd:enumeration value="LoanSeller"/>
			<xsd:enumeration value="LossPayee"/>
			<xsd:enumeration value="MERS"/>
			<xsd:enumeration value="MI Company"/>
			<xsd:enumeration value="MortgageBroker"/>
			<xsd:enumeration value="Notary"/>
			<xsd:enumeration value="NotePayTo"/>
