				<xsd:element name="FRAUD" type="FRAUD" minOccurs="0"/>
				<xsd:element name="LOAN_DELIVERY" type="LOAN_DELIVERY" minOccurs="0"/>
				<xsd:element name="MERS_SERVICE" type="MERS_SERVICE" minOccurs="0"/>
				<xsd:element name="MI" type="MI" minOccurs="0"/>
				<xsd:element name="PRIA" type="PRIA" minOccurs="0"/>
				<xsd:element name="TAX" type="TAX" minOccurs="0"/>
