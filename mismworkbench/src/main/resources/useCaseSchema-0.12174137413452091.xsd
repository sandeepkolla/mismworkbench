			<xsd:enumeration value="CorrespondentLender"/>
			<xsd:enumeration value="Cosigner"/>
			<xsd:enumeration value="CustodianNotePayTo"/>
			<xsd:enumeration value="DeliverRescissionTo"/>
			<xsd:enumeration value="DocumentCustodian"/>
			<xsd:enumeration value="ENoteController"/>
			<xsd:enumeration value="FloodCertificateProvider"/>
