	<xsd:complexType name="BORROWER_DETAIL">
		<xsd:annotation>
			<xsd:appinfo>Additional information about the borrower.</xsd:appinfo>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="BorrowerAgeAtApplicationYearsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The age of the borrower at the time of application in years.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerApplicationSignedDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date that the borrower signed the loan application. Collected on the URLA in Section IX (Borrowers Signature Date).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerBirthDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers date of birth. </xsd:documentation>
					<xsd:appinfo>Collected in lieu of AGE in URLA, Section III, second line, third field.</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerCanadianSocialInsuranceNumberIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The Canadian Social Insurance Number associated with the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerCharacteristicType" type="BorrowerCharacteristicTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the classification applied to a borrower for the purposes of processing or reporting.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerCharacteristicTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The description of the Borrower Characteristic Type when Other is selected as the option from the enumerated list.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerClassificationType" type="BorrowerClassificationTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether the borrower is the primary or a secondary borrower.</xsd:documentation>
					<xsd:appinfo>Replaces "Borrower Print Position Type" from Version 2.x.</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerIsCosignerIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies that the associated Borrower is a Cosigner (only) on the subject loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerMailToAddressSameAsPropertyIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>When true, indicates that the mailing address for the borrower is the same as the property address.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerQualifyingIncomeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The total monthly borrower income per lender or investor guidelines.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerRelationshipTitleType" type="BorrowerRelationshipTitleTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The description of the relationship of a borrower to (an) other borrower(s), if any, for Title purposes.</xsd:documentation>
					<xsd:appinfo> </xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerRelationshipTitleTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>When Other is specified in Borrower Relationship Title Type, this element contains the description.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerSameAsBuilderIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>When true, indicates that the Borrower is also the Builder on the loan transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerTotalMortgagedPropertiesCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of 1- to 4-unit properties that are financed and owned and/or obligated on by an individual borrower. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CreditFileBorrowerAgeYearsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The age of the borrower reported in the credit file returned from the repository bureau. The age is sometimes reported in lieu of the date of birth.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CreditReportAuthorizationIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that the borrower gave the lender consent to pull credit report.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CreditReportIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A reference number assigned by the credit bureau to a specific credit report. This report number is also referenced when a Reissue, Upgrade, or Status Query of an existing report is requested.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DependentCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of dependents disclosed by the borrower. Collected on the URLA in Section III (Dependents No.).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="JointAssetLiabilityReportingType" type="JointAssetLiabilityReportingTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A statement made by two borrowers on a single loan application indicating whether their reporting of assets and liabilities is being completed jointly or not jointly. Collected on the URLA in Section VI (Completed Jointly / Not Jointly - Check Box).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MaritalStatusType" type="MaritalStatusTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The marital status of the party as disclosed by the party.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SchoolingYearsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of years the borrower has attended school. Collected on the URLA in Section III (Yrs. School).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SignedAuthorizationToRequestTaxRecordsIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that the IRS form (4506 or 4506T) that gives the lender the authority to request copies of tax records is present and has been signed by the borrower(s).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SSNCertificationIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether the Social Security # has been certified.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxRecordsObtainedIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that the IRS form (4506 or 4506T) that gives the lender the authority to request copies of tax records has been executed by the lender.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
