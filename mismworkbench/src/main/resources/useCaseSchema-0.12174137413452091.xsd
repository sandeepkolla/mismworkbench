			<xsd:enumeration value="Estate"/>
			<xsd:enumeration value="GovernmentEntity"/>
			<xsd:enumeration value="Guardianship"/>
			<xsd:enumeration value="Individual">
				<xsd:annotation>
					<xsd:documentation>Ownership will be held by one individual.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="JointVenture"/>
			<xsd:enumeration value="LimitedLiabilityCompany"/>
			<xsd:enumeration value="LimitedPartnership"/>
			<xsd:enumeration value="NonProfitCorporation"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Partnership">
