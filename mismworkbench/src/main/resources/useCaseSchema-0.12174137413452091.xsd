			<xsd:documentation>Holds data on multiple loans transmitted as a pool.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="POOL_DETAIL" type="POOL_DETAIL" minOccurs="0"/>
			<xsd:element name="POOL_CERTIFICATE" type="POOL_CERTIFICATE" minOccurs="0">
