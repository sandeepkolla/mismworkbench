			<xsd:enumeration value="Requested">
				<xsd:annotation>
					<xsd:documentation>Borrower or lender selected parameters for searching for a loan product</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Selected">
				<xsd:annotation>
					<xsd:documentation>Loan parameters that will be processed or has been processed. Assumed value if SelectionType is not present </xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Comparison">
				<xsd:annotation>
					<xsd:documentation>One of possibly many loan parameter sets used to complete GFE Page 3</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ServicePaymentMethodTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>This enumerated attribute identifies the type of credit card or other payment method.</xsd:documentation>
			<xsd:appinfo>Renamed.  Used to be "Service Payment Method Type".</xsd:appinfo>
			<xsd:documentation>term: Service Payment Credit Method Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AmericanExpress"/>
			<xsd:enumeration value="DinersClub"/>
			<xsd:enumeration value="Discover"/>
			<xsd:enumeration value="MasterCard"/>
			<xsd:enumeration value="MasterCardDebit"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Visa"/>
			<xsd:enumeration value="VisaDebit"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ServicerTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies a type of Servicer.</xsd:documentation>
			<xsd:documentation>term: Servicer Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
