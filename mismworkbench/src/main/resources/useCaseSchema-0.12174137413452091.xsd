			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Payee"/>
			<xsd:enumeration value="PowerOfAttorney"/>
			<xsd:enumeration value="PreparedBy"/>
			<xsd:enumeration value="PropertyOwner"/>
			<xsd:enumeration value="PropertySeller"/>
			<xsd:enumeration value="ReceivingParty"/>
			<xsd:enumeration value="RealEstateAgent"/>
			<xsd:enumeration value="RegulatoryAgency"/>
			<xsd:enumeration value="RequestingParty"/>
			<xsd:enumeration value="RespondToParty"/>
			<xsd:enumeration value="RespondingParty"/>
			<xsd:enumeration value="ReviewAppraiser"/>
			<xsd:enumeration value="ServiceProvider"/>
