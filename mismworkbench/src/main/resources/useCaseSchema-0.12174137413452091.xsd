			<xsd:documentation>This enumerated attribute identifies what agency the transaction is intended for</xsd:documentation>
			<xsd:documentation>term: Recording Transaction Identifier Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CourtCase">
				<xsd:annotation>
					<xsd:documentation>A transaction in the Clerk of Circuit Court's office</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Recorder">
				<xsd:annotation>
					<xsd:documentation>A transaction in the County Recorder's office</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TitleEscrow">
				<xsd:annotation>
					<xsd:documentation>A transaction with a Title Company or Escrow Agent</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>A transaction with an agency not included in the enumerated list</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
