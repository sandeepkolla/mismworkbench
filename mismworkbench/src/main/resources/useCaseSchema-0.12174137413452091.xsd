			<xsd:element name="CompensationAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of compensation paid to the broker and/or lender in the loan transaction.</xsd:documentation>
					<xsd:appinfo> HUD-1</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CompensationPaidByType" type="CompensationPaidByTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the party(ies) responsible for paying the compensation amount due the broker and/or lender in the loan transaction.</xsd:documentation>
					<xsd:appinfo> HUD-1</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CompensationPaidToType" type="CompensationPaidToTypeEnumerated" minOccurs="0">
